{
	"name": "billing-app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"prepare": "pnpm run smui-theme-light && pnpm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme -i ./node_modules",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark -i ./node_modules"
	},
	"devDependencies": {
		"@material/animation": "^14.0.0",
		"@material/base": "^14.0.0",
		"@material/button": "^14.0.0",
		"@material/card": "^14.0.0",
		"@material/density": "^14.0.0",
		"@material/dialog": "^14.0.0",
		"@material/dom": "^14.0.0",
		"@material/elevation": "^14.0.0",
		"@material/focus-ring": "^14.0.0",
		"@material/ripple": "^14.0.0",
		"@material/rtl": "^14.0.0",
		"@material/shape": "^14.0.0",
		"@material/theme": "^14.0.0",
		"@material/tokens": "^14.0.0",
		"@material/touch-target": "^14.0.0",
		"@material/typography": "^14.0.0",
		"@playwright/test": "^1.28.1",
		"@smui-extra/accordion": "7.0.0-beta.15",
		"@smui-extra/autocomplete": "7.0.0-beta.15",
		"@smui/button": "7.0.0-beta.15",
		"@smui/card": "7.0.0-beta.15",
		"@smui/checkbox": "7.0.0-beta.15",
		"@smui/common": "7.0.0-beta.15",
		"@smui/dialog": "7.0.0-beta.15",
		"@smui/form-field": "7.0.0-beta.15",
		"@smui/icon-button": "7.0.0-beta.15",
		"@smui/radio": "7.0.0-beta.15",
		"@smui/select": "7.0.0-beta.15",
		"@smui/snackbar": "7.0.0-beta.15",
		"@smui/switch": "7.0.0-beta.15",
		"@smui/textfield": "7.0.0-beta.15",
		"@smui/tooltip": "7.0.0-beta.15",
		"@smui/top-app-bar": "7.0.0-beta.15",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"smui-theme": "7.0.0-beta.15",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"sveltekit-superforms": "^1.8.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"vitest": "^0.32.2",
		"zod": "^3.22.4"
	},
	"type": "module",
	"dependencies": {
		"@material/feature-targeting": "^14.0.0",
		"mongoose": "^7.6.3",
		"sass": "^1.69.5"
	}
}
